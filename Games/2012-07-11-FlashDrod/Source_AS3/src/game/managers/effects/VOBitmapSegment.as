package game.managers.effects{
    public class VOBitmapSegment{
        public var x:uint;
        public var y:uint;
        public var width:uint;
        public var height:uint;
        
        public function VOBitmapSegment(x:uint, y:uint, width:uint, height:uint){
            this.x      = x;
            this.y      = y;
            this.width  = width;
            this.height = height;
        }
    }
}
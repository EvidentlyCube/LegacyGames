        public static function autoTile(levelWidth:uint, levelHeight:uint, tiles:Vector.<uint>, autoTileGroups:LevelTilesArray):void {
            _tempAutoTileGroups = autoTileGroups;

            for(var x:uint = 0; x < levelWidth; x++){
                for(var y:uint = 0; y < levelHeight; y++){
                    var tile:int = tiles[x + y * levelWidth];
                    var autoTileGroup:int = _tempAutoTileGroups.getTile(x, y);

                    if (Tiles.isFourWayAutoTile(tile)) {
                        update4WayAutoTile(tile, autoTileGroup, x, y, tiles, levelWidth, levelHeight);
                    } else if (Tiles.isEightWayAutoTile(tile)){
                        update8WayAutoTile(tile, autoTileGroup, x, y, tiles, levelWidth, levelHeight);
                    } else if (tile === Tiles.PIPE_AUOTILE){
                        updatePipeAutoTile(tile, autoTileGroup, x, y, tiles, levelWidth, levelHeight);
                    }
                }
            }
        }

        private static function update4WayAutoTile(tile:uint, autoTileGroup:uint, x:uint, y:uint, tiles:Vector.<uint>, levelWidth:uint, levelHeight:uint):void{
            var currentTileGroup:uint = _autoTileGroups[tile];
            var surroundings:Vector.<uint> = get4WayTiles(x, y, tiles, levelWidth, levelHeight, currentTileGroup, autoTileGroup);

            tiles[x + y * levelWidth] = _autoTileOffset[tile] + _4wayOffsetCalculated[
                surroundings[0] + (surroundings[1] << 1) + (surroundings[2] << 2) + (surroundings[3] << 3)
                ];
        }

        private static function update8WayAutoTile(tile:uint, autoTileGroup:uint, x:uint, y:uint, tiles:Vector.<uint>, levelWidth:uint, levelHeight:uint):void{
            tiles[x + y * levelWidth] = _autoTileOffset[tile] + _8wayOffsetCalculated[
                    get8WayTileIndex(x, y, tiles, levelWidth, levelHeight, _autoTileGroups[tile], autoTileGroup)
            ];
        }

        private static function updatePipeAutoTile(tile:uint, autoTileGroup:uint, x:uint, y:uint, tiles:Vector.<uint>, levelWidth:uint, levelHeight:uint):void{
            var currentTileGroup:uint = _autoTileGroups[tile];
            var surroundings:Vector.<uint> = get4WayTiles(x, y, tiles, levelWidth, levelHeight, currentTileGroup, autoTileGroup);

            tiles[x + y * levelWidth] = _autoTileOffset[tile] + _pipeOffsetCalculated[
                surroundings[0] + (surroundings[1] << 1) + (surroundings[2] << 2) + (surroundings[3] << 3)
                ];
        }

        private static function get4WayTiles(x:uint, y:uint, tiles:Vector.<uint>, levelWidth:uint, levelHeight:uint, tileGroupToCompare:uint, autoTileGroup:uint):Vector.<uint>{
            _tempVectorUint4[0] = y > 0
                ? ( _autoTileGroups[tiles[x + (y - 1) * levelWidth]] === tileGroupToCompare && _tempAutoTileGroups.getTile(x, y - 1) === autoTileGroup ? 1 : 0)
                : 1;
            _tempVectorUint4[1] = x < levelWidth - 1
                ? (_autoTileGroups[tiles[x + 1 + y * levelWidth]] === tileGroupToCompare && _tempAutoTileGroups.getTile(x + 1, y) === autoTileGroup ? 1 : 0)
                : 1;
            _tempVectorUint4[2] = y < levelHeight - 1
                ? (_autoTileGroups[tiles[x + (y + 1) * levelWidth]] === tileGroupToCompare && _tempAutoTileGroups.getTile(x, y + 1) === autoTileGroup ? 1 : 0)
                : 1;
            _tempVectorUint4[3] = x > 0
                ? (_autoTileGroups[tiles[x - 1 + y * levelWidth]] === tileGroupToCompare && _tempAutoTileGroups.getTile(x - 1, y) === autoTileGroup ? 1 : 0)
                : 1;

